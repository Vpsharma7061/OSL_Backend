<table style="width: 100%; border-collapse: collapse; margin: 20px 0; font-size: 16px; text-align: left;">
  <thead>
    <tr>
      {% for header in header %}
        <th style="border: 1px solid #ddd; padding: 8px; background-color: #f2f2f2; color: #333;">{{ header }}</th>
      {% endfor %}
    </tr>
  </thead>
  <tbody>
    {% for row in rows %}
      <tr>
        {% for cell in row %}
          <td style="border: 1px solid #ddd; padding: 8px;">{{ cell }}</td>
        {% endfor %}
      </tr>
    {% endfor %}
  </tbody>
</table>
